html, body {
  background-color: #FFFFFF;
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}


/* Index Garden Home Page */

.coltop  {
  display: flex;
  flex-wrap: wrap;
  margin-right: 20px;
  margin-left: 20px;
}

.img_presentation {
  height: 100px;
}

.card-img-top {
  width: 100%;
  height: 15vw;
  object-fit: cover;
}

.bottom-card-info {
  display: flex;
  justify-content: space-between;
}

.picture-garden-index {
  border-radius: 5px 5px 0px 0px;
  height: 200px;
  object-fit: cover;
  margin-bottom: 20px;
}

/* map */
@media only screen and (min-width: 1100px) {
  .container-map {
    display: grid;
    grid-template-columns: 7fr 4fr;
  }

  .list-map {
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    grid-column-gap: 30px;
    padding: 0 30px;
  }

  #map {
    position: sticky!important;
    top: 0;
    right: 0;
    left: 0;
    height: 100vh;
  }
}

